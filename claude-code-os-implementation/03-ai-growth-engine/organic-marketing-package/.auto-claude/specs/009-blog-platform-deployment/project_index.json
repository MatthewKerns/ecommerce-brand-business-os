{
  "project_type": "monorepo",
  "services": {
    "content-agents": {
      "path": "./claude-code-os-implementation/03-ai-growth-engine/organic-marketing-package/content-agents",
      "tech_stack": ["python", "fastapi", "anthropic"],
      "port": 8000,
      "dev_command": "python -m uvicorn api.main:app --reload --port 8000",
      "test_command": "pytest"
    },
    "dashboard": {
      "path": "./claude-code-os-implementation/03-ai-growth-engine/organic-marketing-package/dashboard",
      "tech_stack": ["typescript", "nextjs", "react", "tailwindcss"],
      "port": 3000,
      "dev_command": "npm run dev",
      "build_command": "npm run build",
      "test_command": "npm test"
    }
  },
  "infrastructure": {
    "docker": false,
    "database": "none",
    "hosting": "vercel_or_netlify",
    "cms": "to_be_determined"
  },
  "conventions": {
    "linter": "eslint",
    "formatter": "prettier",
    "testing": "jest",
    "styling": "tailwindcss"
  },
  "existing_domain": "infinitycards.com",
  "blog_url_target": "infinitycards.com/blog"
}
